script/source = "extends Control

@onready var go_sounds = %goSounds

@onready var animation_player = %AnimationPlayer

@onready var scene_trasition: CanvasLayer = $\"../SceneTrasition\"

@onready var sound_in_out_controller = %soundInOutController

func _on_touch_screen_button_3_pressed():
	get_tree().change_scene_to_file(\"res://scene/mainMenu/mainMenu.tscn\")

func _on_scene_trasition_transitioned() -> void:
	Global.point = 0
	get_tree().change_scene_to_file(\"res://scene/main/main.tscn\")

func _on_blue_car_tsb_pressed():
	Global.car_img = load(\"res://imgs/cars/blueCar.png\")
	sound_in_out_controller.play(\"out\")
	animation_player.play(\"blueChoosen\")
	go_sounds.play()
	scene_trasition.transition()

func _on_aqua_car_tsb_pressed():
	Global.car_img = load(\"res://imgs/cars/aquaCar.png\")
	sound_in_out_controller.play(\"out\")
	animation_player.play(\"aquaChoosen\")
	go_sounds.play()
	scene_trasition.transition()

func _on_red_car_tsb_pressed():
	Global.car_img = load(\"res://imgs/cars/redCar.png\")
	sound_in_out_controller.play(\"out\")
	animation_player.play(\"redChoosen\")
	go_sounds.play()
	scene_trasition.transition()

func _on_white_car_tsb_pressed():
	Global.car_img = load(\"res://imgs/cars/whiteCar.png\")
	sound_in_out_controller.play(\"out\")
	animation_player.play(\"whiteChoosen\")
	go_sounds.play()
	scene_trasition.transition()
	
